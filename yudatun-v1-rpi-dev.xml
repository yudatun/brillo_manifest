<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <include name="default.xml" />

  <remove-project name="device/generic/brillo" />

  <!-- Intel SoCs and Peripherals -->
  <remove-project name="platform/hardware/broadcom/libbt" />

  <project path="device/generic/brillo"
           name="yudatun/brillo_device_generic_brillo"
           remote="yudatun"
           groups="bdk" />

  <!-- Yudatun open-source projects -->
  <project path="Documentation"
           name="yudatun/Documentation"
           remote="yudatun" />
  <project path="manifest"
           name="yudatun/brillo_manifest"
           remote="yudatun" />
  <project path="system/tools/pt-box"
           name="yudatun/system_tools_pt-box"
           remote="yudatun" />
  <project path="thirdparty/dosfstools"
           name="yudatun/thirdparty_dosfstools"
           remote="yudatun" />
  <project path="thirdparty/mtools"
           name="yudatun/thirdparty_mtools"
           remote="yudatun" />
  <project path="thirdparty/wiringPi"
           name="yudatun/thirdparty_wiringPi"
           remote="yudatun" />

  <!-- BEGIN starter boards -->
  <!-- Raspberrypi -->
  <project path="bootable/diskinstaller"
           name="yudatun/bootable_diskinstaller_raspberrypi"
           remote="yudatun" />
  <project path="device/rpi/raspberrypi3"
           name="yudatun/device_rpi_raspberrypi3"
           remote="yudatun" />
  <project path="device/rpi/raspberrypi-common"
           name="yudatun/device_rpi_raspberrypi-common"
           remote="yudatun" />
  <project path="hardware/broadcom/libbt"
           name="platform/hardware/broadcom/libbt"
           groups="raspberrypi" />
  <project path="hardware/bsp/bcm"
           name="yudatun/hardware_bsp_bcm"
           remote="yudatun" />
  <project path="hardware/bsp/kernel/bcm/linux"
           name="raspberrypi/linux"
           remote="raspberrypi"
           revision="rpi-4.9.y" />
  <project path="vendor/raspberrypi/firmware"
           name="yudatun/vendor_raspberrypi_firmware"
           remote="yudatun" />
  <!-- END starter boards -->

  <!-- Brillo product examples -->
  <project path="product/yudatun/yudatun-smartcar"
           name="yudatun/product_yudatun_yudatun-smartcar"
           remote="yudatun" />
</manifest>
